{
    "name": "aurelia-cli-app",
    "type": "project:application",
    "transpiler": {
        "id": "typescript",
        "displayName": "TypeScript",
        "fileExtension": ".ts",
        "dtsSource": [
            "./custom_typings/**/*.d.ts"
        ],
        "source": "src/app/**/*.ts"
    },
    "markupProcessor": {
        "id": "none",
        "displayName": "None",
        "fileExtension": ".html",
        "source": "src/app/**/*.html"
    },
    "cssProcessor": {
        "id": "sass",
        "displayName": "Sass",
        "fileExtension": ".scss",
        "source": "src/app/**/*.scss"
    },
    "editor": {
        "id": "vscode",
        "displayName": "Visual Studio Code"
    },
    "platform": {
        "id": "default",
        "displayName": "Default",
        "output": "scripts",
        "root": "src/**/*.*"
    },
    "run": {
        "basePath": "dist"
    },
    "workflow": null,
    "defaultOrCustom": "custom",
    "copyVendorCss": {
        "output": "dist/css",
        "filename": "vendor-styles.css",
        "sources": [
            "node_modules/mdi/css/*.min.css",
            "node_modules/bootstrap/dist/css/bootstrap.min.css",
            "node_modules/propellerkit/dist/css/propeller.min.css"
        ]
    },
    "paths": {
        "root": "src",
        "app": "app",
        "config": "app/config",
        "resources": "app/resources",
        "elements": "app/resources/elements",
        "attributes": "app/resources/attributes",
        "valueConverters": "app/resources/value-converters",
        "bindingBehaviors": "app/resources/binding-behaviors"
    },
    "build": {
        "copyFiles": {
            "src/app/assets/images/**/*.*": "dist/images",
            "node_modules/mdi/fonts/*": "dist/fonts",
            "src/app/assets/fonts/*": "dist/fonts",
            "src/*.*": "dist"
        },
        "targets": [{
            "id": "web",
            "displayName": "Web",
            "baseUrl": "scripts",
            "output": "dist/scripts"
        }],
        "loader": {
            "type": "require",
            "configTarget": "vendor-bundle.js",
            "includeBundleMetadataInConfig": "auto",
            "plugins": [{
                "name": "text",
                "extensions": [
                    ".html",
                    ".css"
                ],
                "stub": true
            }]
        },
        "options": {
            "minify": {
                "prod": {
                    "mangle": {
                        "keep_fnames": true,
                        "toplevel": true
                    }
                },
                "stage": {}
            },
            "sourcemaps": "dev & stage"
        },
        "bundles": [{
                "name": "app-bundle.js",
                "source": [
                    "[**/*.js]",
                    "**/*.{css,html}"
                ]
            },
            {
                "name": "vendor-bundle.js",
                "prepend": [
                    "node_modules/core-js/client/core.min.js",
                    "node_modules/js-polyfills/polyfill.min.js",
                    "node_modules/bluebird/js/browser/bluebird.core.min.js",
                    "custom_scripts/configure-bluebird.js",
                    "node_modules/requirejs/require.js"
                ],
                "dependencies": [
                    "aurelia-binding",
                    "aurelia-bootstrapper",
                    "aurelia-dependency-injection",
                    "aurelia-event-aggregator",
                    "aurelia-framework",
                    "aurelia-history",
                    "aurelia-history-browser",
                    "aurelia-loader",
                    "aurelia-loader-default",
                    "aurelia-logging",
                    "aurelia-logging-console",
                    "aurelia-metadata",
                    "aurelia-pal",
                    "aurelia-pal-browser",
                    "aurelia-path",
                    "aurelia-polyfills",
                    "aurelia-route-recognizer",
                    "aurelia-router",
                    "aurelia-task-queue",
                    "aurelia-http-client",
                    "aurelia-templating",
                    "aurelia-templating-binding",
                    {
                        "name": "text",
                        "path": "../node_modules/requirejs-text/text"
                    },
                    {
                        "name": "aurelia-templating-resources",
                        "path": "../node_modules/aurelia-templating-resources/dist/amd",
                        "main": "aurelia-templating-resources"
                    },
                    {
                        "name": "aurelia-templating-router",
                        "path": "../node_modules/aurelia-templating-router/dist/amd",
                        "main": "aurelia-templating-router"
                    },
                    {
                        "name": "aurelia-dialog",
                        "path": "../node_modules/aurelia-dialog/dist/amd",
                        "main": "aurelia-dialog"
                    },
                    {
                        "name": "aurelia-configuration",
                        "path": "../node_modules/aurelia-configuration/dist/amd",
                        "main": "index"
                    },
                    "moment",
                    {
                        "name": "aurelia-notify",
                        "path": "../node_modules/aurelia-notify/dist/amd",
                        "main": "aurelia-notify",
                        "resources": [
                            "bs-notification.html",
                            "style.css"
                        ]
                    }
                ]
            },
            {
                "name": "xrx-bundle.js",
                "source": [
                    "[**/*.js]",
                    "**/*.{css,html}"
                ],
                "dependencies": [{
                        "name": "XRXWebservices",
                        "path": "../XRXJSWebservices/XRXWebservices",
                        "deps": [
                            "XRXXmlHandler",
                            "XRXSession",
                            "XRXTemplate",
                            "XRXScanV2",
                            "XRXJobManagement",
                            "XRXDeviceConfig",
                            "XRXUtilities",
                            "XRXPrint",
                            "XRXWsSnmp"
                        ]
                    },
                    {
                        "name": "XRXXmlHandler",
                        "path": "../XRXJSWebservices/XRXXmlHandler"
                    },
                    {
                        "name": "XRXSession",
                        "path": "../XRXJSWebservices/XRXSession"
                    },
                    {
                        "name": "XRXTemplate",
                        "path": "../XRXJSWebservices/XRXTemplate"
                    },
                    {
                        "name": "XRXScan",
                        "path": "../XRXJSWebservices/XRXScan"
                    },
                    {
                        "name": "XRXScanV2",
                        "path": "../XRXJSWebservices/XRXScanV2",
                        "deps": [
                            "XRXScan"
                        ]
                    },
                    {
                        "name": "XRXJobManagement",
                        "path": "../XRXJSWebservices/XRXJobManagement"
                    },
                    {
                        "name": "XRXDeviceConfig",
                        "path": "../XRXJSWebservices/XRXDeviceConfig"
                    },
                    {
                        "name": "XRXUtilities",
                        "path": "../XRXJSWebservices/XRXUtilities"
                    },
                    {
                        "name": "XRXPrint",
                        "path": "../XRXJSWebservices/XRXPrint"
                    },
                    {
                        "name": "XRXWsSnmp",
                        "path": "../XRXJSWebservices/XRXWsSnmp"
                    },
                    {
                        "name": "XRXMassStorage",
                        "path": "../XRXJSWebservices/XRXMassStorage"
                    }
                ]
            }
        ]
    }
}